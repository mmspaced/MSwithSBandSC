  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "bashdb",
      "request": "launch",
      "name": "Bash-Debug (type in script name)",
      "cwd": "${workspaceFolder}",
      "program": "${command:AskForScriptName}",
      "args": []
    },
  {
    "type": "bashdb",
    "request": "launch",
    "name": "Bash-Debug (select script from list of sh files)",
    "cwd": "${workspaceFolder}",
    "program": "${command:SelectScriptName}",
    "args": []
  },
    {
      "type": "java",
      "name": "Launch EurekaserverApplication",
      "request": "launch",
      "mainClass": "se.magnus.eurekaserver.EurekaserverApplication",
      "projectName": "eureka-server"
    },
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch ProductCompositeServiceApplication",
      "request": "launch",
      "mainClass": "se.magnus.microservices.composite.product.ProductCompositeServiceApplication",
      "projectName": "product-composite-service"
    },
    {
      "type": "java",
      "name": "Launch ProductServiceApplication",
      "request": "launch",
      "mainClass": "se.magnus.microservices.core.product.ProductServiceApplication",
      "projectName": "product-service"
    },
    {
      "type": "java",
      "name": "Launch RecommendationServiceApplication",
      "request": "launch",
      "mainClass": "se.magnus.microservices.core.recommendation.RecommendationServiceApplication",
      "projectName": "recommendation-service"
    },
    {
      "type": "java",
      "name": "Launch ReviewServiceApplication",
      "request": "launch",
      "mainClass": "se.magnus.microservices.core.review.ReviewServiceApplication",
      "projectName": "review-service"
    }
  ]
}